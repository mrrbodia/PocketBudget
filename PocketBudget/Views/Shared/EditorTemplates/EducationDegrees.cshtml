@model EducationDegreesViewModel
<div>
    @Html.LabelFor(m => m)
    <button class="btn grey btn-flat edit-education" data-url="@Url.Action("GetDegrees", "Education")">Редагувати</button>
</div>
<div class="education-degrees">
    @if (Model != null && Model.Degrees.Any())
    {
        for (int i = 0; i < Model.Degrees.Count; ++i)
        {
            <div class="row form-row">
                @Html.EditorFor(m => m.Degrees[i])
            </div>
        }
    }
</div>