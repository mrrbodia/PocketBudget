@model List<EducationDegreeViewModel>
<div>
    @Html.LabelFor(m => m)
    <button class="btn grey btn-flat edit-education" data-url="@Url.Action("GetDegrees", "Education")">Редагувати</button>
</div>
<div class="education-degrees">
    @for (int i = 0; i < Model.Count; ++i)
    {
        <div class="row form-row">
            @Html.EditorFor(m => m[i])
        </div>
    }
</div>